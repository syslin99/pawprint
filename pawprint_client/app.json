{
  "expo": {
    "name": "pawprint_client",
    "slug": "pawprint_client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.pawprint_client"
    },
    "androidStatusBar": {
      "backgroundColor": "#202A30",
      "translucent": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#202A30"
        }
      ],
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "colorAccent": {
                "light": "#202A30"
              },
              "colorControlActivated": {
                "light": "#3D84AE"
              },
              "textColorPrimary": {
                "light": "#202A30"
              }
            },
            "timePicker": {
              "headerBackground": {
                "light": "#202A30"
              },
              "numbersSelectorColor": {
                "light": "#3D84AE"
              },
              "numbersTextColor": {
                "light": "#202A30"
              }
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
